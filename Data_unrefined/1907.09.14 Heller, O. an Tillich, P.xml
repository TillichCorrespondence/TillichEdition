   <text type="postcard">
      <body>
         <div type="writingSession">
            <pb n="1"/>

            <p>Ni à <placeName ref="Paris">Paris</placeName>, ni à <placeName ref="London"
                  >Londres</placeName>, ni à <placeName ref="Rom">Rome</placeName>, mais à
                  <placeName ref="Nancy">Nancy</placeName>! Mes remerciements pour votre carte! </p>
            <closer>
               <salute>Toujours ton</salute>
               <signed><hi rend="u"><persName ref="P02142">O. Heller</persName></hi></signed>
            </closer>

            <postscript>
               <p>8st. Berufsarbeiter a. D.</p>
               <p>z. Z. Vagabund i. Frankreich.</p>
            </postscript>

         </div>
         <div type="ZweiteHandschrift">
            <p>Dein alter Bekannter</p>
            <closer><signed><unclear>¿¿¿</unclear></signed></closer>
         </div>
      </body>
